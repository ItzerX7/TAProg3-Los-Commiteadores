	<parameter name="InputGroupID" class="java.lang.String"/>
	<query language="sql"><![CDATA[SELECT g.nombre AS NOMBREGRUPO,
	d.nombre AS NOMBREDISPOS,
	d.dispositivoid AS IDDISPOS,
	d.fecharegistro AS FECHADISPOS,
	d.estado_conexion AS ESTADODISPOS,
	count(*) as cantidad
FROM taprog3.dispositivo d,
	taprog3.grupo g
WHERE 
	 d.grupo_grupoid =  $P{InputGroupID} 
	 AND d.grupo_grupoid = g.grupoid
GROUP BY
	d.estado_conexion]]></query>

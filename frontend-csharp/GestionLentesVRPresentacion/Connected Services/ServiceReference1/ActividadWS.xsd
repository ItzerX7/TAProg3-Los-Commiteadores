<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://ws.gestionlentesvr.pucp.com/" targetNamespace="http://ws.gestionlentesvr.pucp.com/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Exception" type="tns:Exception" />
  <xs:element name="actualizarActividad" type="tns:actualizarActividad" />
  <xs:element name="actualizarActividadResponse" type="tns:actualizarActividadResponse" />
  <xs:element name="eliminarActividad" type="tns:eliminarActividad" />
  <xs:element name="eliminarActividadResponse" type="tns:eliminarActividadResponse" />

  <xs:element name="registrarActividadResponse" type="tns:registrarActividadResponse" />
  <xs:complexType name="registrarActividad">
    <xs:sequence>
      <xs:element minOccurs="0" name="actividad" type="tns:actividad" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="actividad">
    <xs:sequence>
      <xs:element name="activo" type="xs:unsignedShort" />
      <xs:element minOccurs="0" name="descripcion" type="xs:string" />
      <xs:element minOccurs="0" name="detallesTecnicos" type="xs:string" />
      <xs:element minOccurs="0" name="dispositivo" type="tns:dispositivo" />
      <xs:element minOccurs="0" name="fechaHora" type="xs:dateTime" />
      <xs:element name="id" type="xs:int" />

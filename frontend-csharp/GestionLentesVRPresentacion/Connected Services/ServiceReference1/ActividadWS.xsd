    <xs:sequence />
  </xs:complexType>
  <xs:simpleType name="estadoConexion">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CONECTADO" />
      <xs:enumeration value="DESCONECTADO" />
      <xs:enumeration value="EN_USO" />
      <xs:enumeration value="EN_MANTENIMIENTO" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tipoActividad">
    <xs:restriction base="xs:string">
      <xs:enumeration value="INICIO_SESION" />
      <xs:enumeration value="CIERRE_SESION" />
      <xs:enumeration value="REGISTRO_DISPOSITIVO" />
      <xs:enumeration value="ACTUALIZACION_FIRMWARE" />
      <xs:enumeration value="INSTALACION_APLICACION" />
      <xs:enumeration value="DESINSTALACION_APLICACION" />
      <xs:enumeration value="CONFIGURACION_DISPOSITIVO" />
      <xs:enumeration value="CAMBIO_ESTADO_DISPOSITIVO" />
      <xs:enumeration value="ASIGNACION_GRUPO" />
      <xs:enumeration value="CREACION_USUARIO" />
      <xs:enumeration value="MODIFICACION_USUARIO" />
      <xs:enumeration value="CREACION_GRUPO" />
      <xs:enumeration value="MODIFICACION_GRUPO" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>